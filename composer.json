{
	"name": "webcoast/versatile-crawler",
	"description": "A versatile and extendable crawler for TYPO3 CMS",
	"type": "typo3-cms-extension",
	"keywords": ["typo3", "crawler", "search"],
	"license": "GPL-3.0+",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Thorben Nissen",
			"email": "thorben@webcoast.dk"
		}
	],
	"extra": {
		"branch-alias": {
			"dev-release/2.0": "2.0.x-dev"
		},
		"typo3/cms": {
			"extension-key": "versatile_crawler"
		}
	},
	"autoload": {
		"psr-4": {
			"WEBcoast\\VersatileCrawler\\": "Classes"
		}
	},
	"require": {
		"typo3/cms-core": "^v12.4"
	}
}
